{
  "name": "manifest-validator",
  "version": "0.3.2",
  "description": "A Node.js based validator for appcache manifest files",
  "keywords": [
    "appcache",
    "offline",
    "cache manifest",
    "validator",
    "validate"
  ],
  "author": "Frederic Hemberger (http://frederic-hemberger.de/)",
  "homepage": "http://manifest-validator.com/",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production PORT=8735 node app/index.js",
    "test": "NODE_ENV=test node_modules/.bin/mocha",
    "coverage": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- -R spec"
  },
  "dependencies": {
    "busboy": "*",
    "express": "^3.4",
    "piwik-tracker": "*",
    "jade": "*",
    "request": "*",
    "extend": "^1.2",
    "js-yaml": "^3.0"
  },
  "devDependencies": {
    "chai": "^1.x",
    "mocha": "^1.x",
    "supertest": "^1.x"
  }
}
